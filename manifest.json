{
    "manifest_version": 2,
    "name": "Diario de Mallorca",
    "description": "Remove premium paywalls from Diario de Mallorca articles",
    "version": "1.0",
    "icons": {
        "16": "images/16.png",
        "32": "images/32.png",
        "48": "images/48.png",
        "128": "images/128.png"
      },
    "content_scripts": [{
	    "matches": ["https://www.diariodemallorca.es/*"],
		"js": ["free-premium.js"]
	}]
  }